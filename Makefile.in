					vf_dev_num=$$(($$dev * $(QAT_DH895XCC_NUM_VFS) + $$vf_dev));						\
					$(INSTALL) -D -m 640 $(ICP_BUILD_OUTPUT)/dh895xccvf_dev0.conf.vm /etc/dh895xccvf_dev$$vf_dev_num.conf;	\
				done;														\
