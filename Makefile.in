	@$(RM) $(bindir)/$(ADF_DBG_DAEMON_CTL_SCR)

###########################
# ADF_CTL related targets #
###########################

adf-ctl-all:
	@$(MAKE) -C $(ADF_CTL_DIR)
	@$(INSTALL) -D -m 750 $(ADF_CTL_DIR)/adf_ctl $(ICP_BUILD_OUTPUT)/$(ADF_CTL_BIN)
	@$(foreach conf, $(CONFIG_LIST), $(INSTALL) -D -m 750 $(QUICKASSIST_DIR)/utilities/adf_ctl/conf_files/$(conf) $(ICP_BUILD_OUTPUT)/$(conf);)
        # c4xxx configs are handled by the config.sh scripts for host only
	@if [ "$(ICP_QAT_DBG)" == "y" ]; then \
		if [ "$(ICP_SRIOV_AM)" == "1" ]; then     \
			$(TAIL) -n +$$((1 + `awk '/^###/,/^###/{n++} {print}; n>1 {exit}' $(ADF_CTL_DIR)/conf_files/qat_dbg_section.slice.vm | wc -l`)) \
				$(ADF_CTL_DIR)/conf_files/qat_dbg_section.slice.vm > $(ICP_BUILD_OUTPUT)/qat_dbg_section.slice.vm; \
			$(FIND) $(ICP_BUILD_OUTPUT) \( -name 'c4xxx*.conf*.vm' -o -name '*.conf.vm' \) -$(EXEC) bash -c ' $(CAT) $(ICP_BUILD_OUTPUT)/qat_dbg_section.slice.vm >> $$0' {} \;;	\
		else	\
			$(TAIL) -n +$$((1 + `awk '/^###/,/^###/{n++} {print}; n>1 {exit}' $(ADF_CTL_DIR)/conf_files/qat_dbg_section.slice | wc -l`)) \
				$(ADF_CTL_DIR)/conf_files/qat_dbg_section.slice > $(ICP_BUILD_OUTPUT)/qat_dbg_section.slice; \
			$(FIND) $(ICP_BUILD_OUTPUT) \( -name 'c4xxx*' -o -name '*.conf' -o -name '*.conf.vm' \) -$(EXEC) bash -c ' $(CAT) $(ICP_BUILD_OUTPUT)/qat_dbg_section.slice >> $$0' {} \;;	\
		fi; \
	fi
	@if [ "$(ICP_DC_ONLY)" == 1 ]; then \
		if [ "$(ICP_QAT_DBG)" == "y" ]; then \
			if [ "$(ICP_SRIOV_AM)" == "1" ]; then     \
				$(FIND) $(ICP_BUILD_OUTPUT) ! -name 'c4xxx*' -name '*.conf.dc.vm' -$(EXEC) bash -c ' $(CAT) $(ICP_BUILD_OUTPUT)/qat_dbg_section.slice.vm >> $$0' {} \;;	\
			else	\
				$(FIND) $(ICP_BUILD_OUTPUT) ! -name 'c4xxx*' -name '*.conf.dc*' -$(EXEC) bash -c ' $(CAT) $(ICP_BUILD_OUTPUT)/qat_dbg_section.slice >> $$0' {} \;; \
			fi; \
		fi; \
		$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.dc' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.dc/.conf}' {} \;;	\
		if [ "$(ICP_SRIOV_AM)" == "1" -o "$(ICP_SRIOV_AM)" == "2" ]; then     \
			$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.dc.vm' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.dc.vm/.conf.vm}' {} \;; \
		fi;  \
	fi
	@if [ "$(ICP_DC_SYM_ONLY_AM)" == 1 ]; then \
		if [ "$(ICP_QAT_DBG)" == "y" ]; then \
			if [ "$(ICP_SRIOV_AM)" == "1" ]; then     \
				$(FIND) $(ICP_BUILD_OUTPUT) ! -name 'c4xxx*' -name '*.conf.dc.sym.vm' -$(EXEC) bash -c ' $(CAT) $(ICP_BUILD_OUTPUT)/qat_dbg_section.slice.vm >> $$0' {} \;;	\
			else	\
				$(FIND) $(ICP_BUILD_OUTPUT) ! -name 'c4xxx*' -name '*.conf.dc.sym*' -$(EXEC) bash -c ' $(CAT) $(ICP_BUILD_OUTPUT)/qat_dbg_section.slice >> $$0' {} \;; \
			fi; \
		fi; \
		$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.dc.sym' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.dc.sym/.conf}' {} \;;	\
		if [ "$(ICP_SRIOV_AM)" == "1" -o "$(ICP_SRIOV_AM)" == "2" ]; then     \
			$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.dc.sym.vm' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.dc.sym.vm/.conf.vm}' {} \;; \
		fi;  \
	fi
	@if [ "$(ICP_KAPI_AM)" == 1 ]; then \
		$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.km' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.km/.conf}' {} \;;	\
		if [ "$(ICP_SRIOV_AM)" == "1" -o "$(ICP_SRIOV_AM)" == "2" ]; then     \
			$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.vm.km' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.vm.km/.conf.vm}' {} \;; \
		fi;  \
	fi
	@if [ "$(ICP_DC_ONLY)" == 1 -a "$(ICP_KAPI_AM)" == 1 ]; then \
		$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.dc.km' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.dc.km/.conf}' {} \;;	\
		if [ "$(ICP_SRIOV_AM)" == "1" -o "$(ICP_SRIOV_AM)" == "2" ]; then     \
			$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.dc.vm.km' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.dc.vm.km/.conf.vm}' {} \;; \
		fi;  \
	fi
	@if [ "$(ICP_DC_SYM_ONLY_AM)" == 1 -a "$(ICP_KAPI_AM)" == 1 ]; then \
		$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.dc.sym.km' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.dc.sym.km/.conf}' {} \;;	\
		if [ "$(ICP_SRIOV_AM)" == "1" -o "$(ICP_SRIOV_AM)" == "2" ]; then     \
			$(FIND) $(ICP_BUILD_OUTPUT) ! -name "c4xxx*" -name '*.conf.dc.sym.vm.km' -$(EXEC) bash -c ' $(MV) $$0 $${0/.conf.dc.sym.vm.km/.conf.vm}' {} \;; \
		fi;  \
	fi

adf-ctl-install:
	@if [ $(isLegacyLoaded) == 1 ] && [ "$(QAT_COEXISTENCE)" == "y" ]; then	\
		if [ "$(ICP_QAT_DBG)" == "y" ]; then \
			$(INSTALL) -D -m 750 $(ADF_CTL_DIR)/$(ADF_CTL_BIN_ORG) $(bindir)/$(ADF_CTL_BIN);	\
		else \
			$(INSTALL) -D -m 750 $(ADF_CTL_DIR)/$(ADF_CTL_BIN) $(bindir)/$(ADF_CTL_BIN);	\
		fi \
	else \
		if [ $(isLegacyLoaded) == 0 ] && [ "$(QAT_COEXISTENCE)" != "y" ]; then \
			if [ "$(ICP_QAT_DBG)" == "y" ]; then \
				$(INSTALL) -D -m 750 $(ADF_CTL_DIR)/$(ADF_CTL_BIN_ORG) $(bindir)/$(ADF_CTL_BIN);	\
			else \
				$(INSTALL) -D -m 750 $(ADF_CTL_DIR)/$(ADF_CTL_BIN) $(bindir)/$(ADF_CTL_BIN);	\
			fi \
		fi; \
	fi

adf-ctl-uninstall:
	@$(RM) $(bindir)/$(ADF_CTL_BIN)

adf-ctl-clean:
	@$(MAKE) clean -C $(ADF_CTL_DIR)

adf-rl-clean:
	@$(MAKE) clean -C $(ADF_RL_DIR)

qat-driver-all:
	@$(MAKE) KDIR=$(KERNEL_SOURCE_ROOT) -C $(QAT_DRIVER_DIR) modules

qat-upstream-check:
	@if [ -f /usr/sbin/qatmgr ] || [ -f /usr/local/sbin/qatmgr ]; then \
		$(ECHO) -e "QAT Upstream package already installed"; \
		exit 1; \
	fi

qat-driver-install:
	@if [ $(isLegacyLoaded) != 0 ] && [ "$(QAT_COEXISTENCE)" != "y" ]; then	\
		$(ECHO) -e "\n\n\t*** icp_qa_al module for DH895x Installed !!! ***";	\
		$(ECHO) -e "\t*** Try configure --enable-qat-coexistence    ***\n\n";	\
	else	\
		if [ $(isLegacyLoaded) == 0 ] && [ "$(QAT_COEXISTENCE)" == "y" ]; then \
			$(ECHO) -e "\n\n\t*** icp_qa_al module for DH895x  NOT  Installed !!! ***";   \
			$(ECHO) -e "\t*** and --enable-qat-coexistence option is used     ***";   \
	                $(ECHO) -e "\t*** Try configure without --enable-qat-coexistence  ***\n\n";   \
		else	\
			if [ -z '$(mod_sign_cmd)' ]; then \
	               		$(ECHO) "No driver signing required."; \
	               		$(MAKE) KDIR=$(KERNEL_SOURCE_ROOT) -C $(QAT_DRIVER_DIR) mod_sign_cmd=":" modules_install; \
			else	\
	               		$(ECHO) "Driver signing is required.";	\
	               		$(MAKE) KDIR=$(KERNEL_SOURCE_ROOT) -C $(QAT_DRIVER_DIR) mod_sign_cmd="$(mod_sign_cmd)" modules_install; \
			fi;	\
		fi; \
	fi

qat-driver-uninstall:

qat-driver-clean:
	@$(MAKE) KDIR=$(KERNEL_SOURCE_ROOT) -C $(QAT_DRIVER_DIR) clean

