osal_build:
	@$(MAKE) -C $(QUICKASSIST_DIR) libosal_user

osal_clean:
	@echo ; echo 'Cleaning OSAL';
	@cd $(QUICKASSIST_DIR/$(OSAL_PATH)/src && $(MAKE) ICP_ENV_DIR=$(ICP_TOP_ENV) ICP_OS_LEVEL=user_space OS_LEVEL=linux osal_clean;

xen_build: qat-driver-all osal_build adf-rl-all adf-ctl-all
	@$(RM) $(ICP_BUILD_OUTPUT)/*.conf*
	@$(foreach ko, $(KO_MODULES_LIST),										\
		if [ -e $(QUICKASSIST_QAT_DIR)/$(ko) ]; then								\
			$(INSTALL) -D -m 750 $(QUICKASSIST_QAT_DIR)/$(ko) $(ICP_BUILD_OUTPUT);				\
		fi;													\
	)
	@$(foreach bin, $(filter qat_895xcc.bin qat_895xcc_mmp.bin qat_c62x.bin qat_c62x_mmp.bin, $(BIN_LIST)),		\
		if [ -e $(QUICKASSIST_QAT_DIR)/fw/$(bin) ]; then							\
			$(INSTALL) -D -m 750 $(QUICKASSIST_QAT_DIR)/fw/$(bin) $(ICP_BUILD_OUTPUT)/$(bin);		\
		fi;													\
	)
	$(INSTALL) -D -m 750 $(QUICKASSIST_DIR)/utilities/adf_ctl/conf_files/*.xen $(ICP_BUILD_OUTPUT)/;

xen_clean: qat-driver-clean adf-rl-clean adf-ctl-clean osal_clean
	@$(RM) $(ICP_BUILD_OUTPUT)

xen_install xen_uninstall:
	@echo ; echo 'Xen installation is not supported on this device';

