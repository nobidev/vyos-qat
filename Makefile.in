	@$(INSTALL) -D -m 750 $(ADF_RL_DIR)/debug/daemon/build/linux_2.6/user_space/qat_dbg_sync_daemon $(ICP_BUILD_OUTPUT)/$(ADF_DBG_DAEMON_BIN)
